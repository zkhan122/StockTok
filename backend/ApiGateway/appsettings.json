{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "dev-p44mo426qxs1apjs.us.auth0.com",
    "Audience": "https://api.stocktok.com"
  },
  "ReverseProxy": {
    "Routes": {
      "userRoute": {
        "RouteId": "userRoute",
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "{ReqHeader:Authorization}"
          }
        ]
      },
      "watchlistRoute": {
        "RouteId": "watchlistRoute",
        "ClusterId": "userCluster",
        "Match": {
          "Path": "/api/watchlists/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "{ReqHeader:Authorization}"
          }
        ]
      },
      "marketRoute": {
        "RouteId": "marketRoute",
        "ClusterId": "marketCluster",
        "Match": {
          "Path": "/api/market/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "userCluster": {
        "Destinations": {
          "user1": {
            "Address": "http://localhost:5168/"
          }
        }
      },
      "marketCluster": {
        "Destinations": {
          "market1": {
            "Address": "http://localhost:8000/"
          }
        }
      }
    }
  }
}